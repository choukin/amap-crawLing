    "nested-formdata": "1.1.0",
    "openai": "4.25.0",
    "playwright": "1.42.1",
    "prompt": "1.3.0",
    "qrcode-terminal": "0.12.0",
    "readline": "1.3.0",
    "redis": "4.6.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "4.7.1"
